#lang sicp
(define (compose g f)
  (lambda (x)
    (g (f x))))

(define (a x)
  (+ x 1))

(define (b x)
  (+ x 2))

;((compose a b) 1)

(define (repeated f n)
  (define init f)
  (lambda (x)
    (if (> n 1)
        (repeated (compose f init) (- n 1))
        ((compose f init) x))))

(define (square x)
  (* x x))

((repeated square 2) 5)




